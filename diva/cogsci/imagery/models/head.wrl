DEF SCRIPT Script {
    eventIn SFBool laugh IS laugh
    eventIn SFBool smile IS smile
    eventIn SFBool frown IS frown
    eventIn SFBool yes IS yes
    eventIn SFBool no IS no

    eventOut SFTime laughTime
    eventOut SFTime smileTime
    eventOut SFTime frownTime
    eventOut SFTime yesTime
    eventOut SFTime noTime
    url "vrmlscript:
        function laugh (value, eventTime) {
            laughTime = eventTime;
        }
        function smile (value, eventTime) {
            smileTime = eventTime;
        }
        function frown (value, eventTime) {
            frownTime = eventTime;
        }
        function yes (value, eventTime) {
            yesTime = eventTime;
        }
        function no (value, eventTime) {
            noTime = eventTime;
        }
        "
}

