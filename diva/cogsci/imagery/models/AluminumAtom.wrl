# interpolators
DEF time TimeSensor	{
	stopTime	-1
	cycleInterval	0.0500
	loop	TRUE
}
# interpolators
DEF ascript Script	{
	eventIn	SFTime	ago
	field	SFFloat	FAR	-25
	eventOut	SFVec3f	kpos1
	eventOut	SFVec3f	kpos2
	field	SFFloat	kradius	10.0000
	field	SFFloat	kangle	1.0000
	field	SFFloat	kspeed	4.0000
	eventOut	SFVec3f	lpos1
	eventOut	SFVec3f	lpos2
	eventOut	SFVec3f	lpos3
	eventOut	SFVec3f	lpos4
	eventOut	SFVec3f	lpos5
	eventOut	SFVec3f	lpos6
	eventOut	SFVec3f	lpos7
	eventOut	SFVec3f	lpos8
	field	SFFloat	lradius	15.0000
	field	SFFloat	langle	1.0000
	field	SFFloat	lspeed	4.5000
	eventOut	SFVec3f	mpos1
	eventOut	SFVec3f	mpos2
	eventOut	SFVec3f	mpos3
	field	SFFloat	mradius	20.0000
	field	SFFloat	mangle	1.0000
	field	SFFloat	mspeed	5.0000
	field	SFFloat	nradius	25.0000
	field	SFFloat	nangle	1.0000
	field	SFFloat	nspeed	5.5000
	field	SFFloat	oradius	30.0000
	field	SFFloat	oangle	1.0000
	field	SFFloat	ospeed	6.0000
	field	SFFloat	pradius	35.0000
	field	SFFloat	pangle	1.0000
	field	SFFloat	pspeed	6.5000
	field	SFFloat	qradius	40.0000
	field	SFFloat	qangle	1.0000
	field	SFFloat	qspeed	7.0000
	url	"vrmlscript:
		function degtorad( deg )	{
			return Math.PI/180 * deg;
		}
		function initialize()	{
			kpos1 = new SFVec3f(0,0,kradius );
			kpos2 = new SFVec3f(0,0,kradius );
			lpos1 = new SFVec3f(0,0,lradius );
			lpos2 = new SFVec3f(0,0,lradius );
			lpos3 = new SFVec3f(0,0,lradius );
			lpos4 = new SFVec3f(0,0,lradius );
			lpos5 = new SFVec3f(0,0,lradius );
			lpos6 = new SFVec3f(0,0,lradius );
			lpos7 = new SFVec3f(0,0,lradius );
			lpos8 = new SFVec3f(0,0,lradius );
			mpos1 = new SFVec3f(0,0,mradius );
			mpos2 = new SFVec3f(0,0,mradius );
			mpos3 = new SFVec3f(0,0,mradius );
			kangle = 0;
			langle = 0;
			mangle = 0;
		}
		function ago(thetime)	{
		kangle += kspeed;
		if ( kangle > 360 )
			kangle = 0;
		langle += lspeed;
		if ( langle > 360 )
			langle = 0;
		mangle += mspeed;
		if ( mangle > 360 )
			mangle = 0;
		kpos1[0] = kradius * Math.sin(degtorad(kangle + 0.0000));
		kpos1[1] = kradius * Math.cos(degtorad(kangle + 0.0000));
		kpos1[2] = 0.0;
		kpos2[0] = kradius * Math.sin(degtorad(kangle + 180.0000));
		kpos2[1] = kradius * Math.cos(degtorad(kangle + 180.0000));
		kpos2[2] = 0.0;
		lpos1[0] = 0.0;
		lpos1[1] = lradius * Math.sin(degtorad(langle + 0.0000));
		lpos1[2] = lradius * Math.cos(degtorad(langle + 0.0000));
		lpos2[0] = 0.0;
		lpos2[1] = lradius * Math.sin(degtorad(langle + 180.0000));
		lpos2[2] = lradius * Math.cos(degtorad(langle + 180.0000));
		lpos3[0] = 0.0;
		lpos3[1] = lradius * Math.sin(degtorad(langle + 90.0000));
		lpos3[2] = lradius * Math.cos(degtorad(langle + 90.0000));
		lpos4[0] = 0.0;
		lpos4[1] = lradius * Math.sin(degtorad(langle + 270.0000));
		lpos4[2] = lradius * Math.cos(degtorad(langle + 270.0000));
		lpos5[0] = 0.0;
		lpos5[1] = lradius * Math.sin(degtorad(langle + 8.0000));
		lpos5[2] = lradius * Math.cos(degtorad(langle + 8.0000));
		lpos6[0] = 0.0;
		lpos6[1] = lradius * Math.sin(degtorad(langle + 188.0000));
		lpos6[2] = lradius * Math.cos(degtorad(langle + 188.0000));
		lpos7[0] = 0.0;
		lpos7[1] = lradius * Math.sin(degtorad(langle + 98.0000));
		lpos7[2] = lradius * Math.cos(degtorad(langle + 98.0000));
		lpos8[0] = 0.0;
		lpos8[1] = lradius * Math.sin(degtorad(langle + 278.0000));
		lpos8[2] = lradius * Math.cos(degtorad(langle + 278.0000));
		mpos1[0] = mradius * Math.sin(degtorad(mangle + 0.0000));
		mpos1[1] = 0.0;
		mpos1[2] = mradius * Math.cos(degtorad(mangle + 0.0000));
		mpos2[0] = mradius * Math.sin(degtorad(mangle + 180.0000));
		mpos2[1] = 0.0;
		mpos2[2] = mradius * Math.cos(degtorad(mangle + 180.0000));
		mpos3[0] = mradius * Math.sin(degtorad(mangle + 90.0000));
		mpos3[1] = 0.0;
		mpos3[2] = mradius * Math.cos(degtorad(mangle + 90.0000));
	}
"
}
#	this runs the script
ROUTE time.cycleTime TO ascript.ago
#	now the electrons
ROUTE ascript.kpos1 TO kshellpos1.set_translation
ROUTE ascript.kpos2 TO kshellpos2.set_translation
ROUTE ascript.lpos1 TO lshellpos1.set_translation
ROUTE ascript.lpos2 TO lshellpos2.set_translation
ROUTE ascript.lpos3 TO lshellpos3.set_translation
ROUTE ascript.lpos4 TO lshellpos4.set_translation
ROUTE ascript.lpos5 TO lshellpos5.set_translation
ROUTE ascript.lpos6 TO lshellpos6.set_translation
ROUTE ascript.lpos7 TO lshellpos7.set_translation
ROUTE ascript.lpos8 TO lshellpos8.set_translation
ROUTE ascript.mpos1 TO mshellpos1.set_translation
ROUTE ascript.mpos2 TO mshellpos2.set_translation
ROUTE ascript.mpos3 TO mshellpos3.set_translation
