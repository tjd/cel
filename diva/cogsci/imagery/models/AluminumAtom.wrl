# Draw nucleus for Aluminum
DEF nucleus Transform	{
	translation 0 0 0
	children [
		DEF proton Transform {
		translation 2.0 0 0
		children [
			Shape	{
				appearance Appearance {
				  material	Material {
					ambientIntensity	0.25
					diffuseColor	0.33 0.34 0.6
					specularColor	0.29 0.29 0.29
					emissiveColor	0 0 0
					shininess	0.79
					transparency	0
				  }
				}
				geometry Sphere {
					radius 3.0000
				}
			}
		]
		}
		DEF neutron Transform {
		translation -2.0 0 0
		children [
			Shape	{
				appearance Appearance {
				  material	Material {
					ambientIntensity	0.259259
					diffuseColor	0.32 0.54 0.26
					specularColor	0.46 0.46 0.46
					emissiveColor	0 0 0
					shininess	0.51
					transparency	0
				  }
				}
				geometry Sphere {
					radius 3.0000
				}
			}
		]
		}
	]
}
