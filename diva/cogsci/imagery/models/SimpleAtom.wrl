#VRML V2.0 utf8

# Draw nucleus for atom
DEF nucleus Transform	{
	translation 0 0 0
	children [
		Shape	{
			appearance Appearance {
			  material	Material {
				ambientIntensity	0.25
				diffuseColor	1 0 0
			  }
			}
			geometry Sphere {
				radius 2.0
			}
		}
	]
}

